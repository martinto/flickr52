<h1><%= @challenge.title %></h1>

<table>
  <thead>
  <tr>
    <th>nsid</th>
    <th>User Name</th>
    <th colspan="2"></th>
  </tr>
  </thead>

  <tbody>
  <% @members.each do |member| %>
      <tr>
        <td><%= member.nsid %></td>
        <td><%= member.username %></td>
        <td><%= link_to 'Show', challenge_member_path(@challenge, member) %></td>
        <td><%= link_to 'Edit', edit_challenge_member_path(@challenge, member) %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
